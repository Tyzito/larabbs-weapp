
<view class="counter {{style}}">
  <button size="mini" data-wpy-evt="14-0" bind:tap="__dispatcher">  +  </button>
  <button size="mini" data-wpy-evt="14-1" bind:tap="__dispatcher">  -  </button>
  <button size="mini" data-wpy-evt="14-2" bind:tap="__dispatcher">  INCREMENT  </button>
  <button size="mini" data-wpy-evt="14-3" bind:tap="__dispatcher">  DECREMENT  </button>
  <button size="mini" data-wpy-evt="14-4" bind:tap="__dispatcher">  ASYNC INCREMENT  </button>
  <label class="count"> {{counter}} </label>
</view>
